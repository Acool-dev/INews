{
  "manifest_version": 3,
  "name": "News Extension",
  "version": "1.0",
  "description": "Fetch and display the latest news articles.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "https://newsapi.org/"
  ]
}
